{
    "extends": ["airbnb", "prettier"],
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "node": true
    },
    "rules": {
        "semi": ["error", "always"],
        // "quotes": "off",
        "no-console": "off",
        "no-unreachable": "off",
        "no-redeclare": "warn"
    }
}